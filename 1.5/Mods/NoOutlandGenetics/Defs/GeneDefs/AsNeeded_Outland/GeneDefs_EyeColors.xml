<?xml version="1.0" encoding="UTF-8"?>
<Defs>

	<GeneDef Name="OutlandGeneEyeColor" Abstract="True">
		<displayCategory>Cosmetic</displayCategory>
		<biostatCpx>0</biostatCpx>
		<exclusionTags>
			<li>EyeColor</li>
		</exclusionTags>
		<iconPath>OuterRim/GeneIcons/Gene_Eyes</iconPath>
		<graphicData>
			<color>(255, 255, 255)</color>
			<visibleNorth>false</visibleNorth>
			<layer>PostTattoo</layer>
			<graphicPath>OuterRim/Genes/Eyes/Eyes_Male</graphicPath>
			<graphicPathFemale>OuterRim/Genes/Eyes/Eyes_Female</graphicPathFemale>
		</graphicData>
		<modExtensions>
			<li Class="VanillaGenesExpanded.GeneExtension">
				<backgroundPathEndogenes>OuterRim/GeneIcons/EndogeneBG</backgroundPathEndogenes>
				<backgroundPathXenogenes>OuterRim/GeneIcons/XenogeneBG</backgroundPathXenogenes>
			</li>
		</modExtensions>
	</GeneDef>

	<GeneDef ParentName="OutlandGeneEyeColor">
		<defName>Outland_Eye_Orange</defName>
		<label>orange eyes</label>
		<labelShortAdj>orange-eyed</labelShortAdj>
		<description>Carriers of this gene have deeply orange eyes.</description>
		<iconColor>(249, 128, 29)</iconColor>
		<graphicData>
			<color>(249, 128, 29)</color>
		</graphicData>
		<displayOrderInCategory>7801</displayOrderInCategory>
	</GeneDef>

	<GeneDef ParentName="OutlandGeneEyeColor">
		<defName>Outland_Eye_White</defName>
		<label>white eyes</label>
		<labelShortAdj>white-eyed</labelShortAdj>
		<description>Carriers of this gene have blazing white eyes.</description>
		<iconColor>(249, 255, 255)</iconColor>
		<graphicData>
			<color>(249, 255, 255)</color>
		</graphicData>
		<displayOrderInCategory>7812</displayOrderInCategory>
	</GeneDef>

</Defs>