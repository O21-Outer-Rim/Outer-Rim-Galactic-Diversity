<?xml version="1.0" encoding="UTF-8"?>
<Defs>

	<GeneDef ParentName="GeneHealingRateBase">
		<defName>Outland_Regeneration</defName>
		<label>regeneration</label>
		<description>Carriers of this gene heal from wounds faster than normal and even regenerate limbs.</description>
		<iconPath>OuterRim/GeneIcons/Gene_Regeneration</iconPath>
		<displayOrderInCategory>7900</displayOrderInCategory>
		<geneClass>TabulaRasa.Gene_Regeneration</geneClass>
		<statFactors>
			<InjuryHealingFactor>1.25</InjuryHealingFactor>
		</statFactors>
		<biostatMet>-2</biostatMet>
		<modExtensions>
			<li Class="VanillaGenesExpanded.GeneExtension">
				<backgroundPathEndogenes>OuterRim/GeneIcons/EndogeneBG</backgroundPathEndogenes>
				<backgroundPathXenogenes>OuterRim/GeneIcons/XenogeneBG</backgroundPathXenogenes>
			</li>
			<li Class="TabulaRasa.DefModExt_GeneRegeneration">
				<healTicks>1200</healTicks>
				<regrowParts>true</regrowParts>
				<cureTicks>300</cureTicks>
				<removeInfections>true</removeInfections>
				<infectionsAllowed />
				<explicitRemovals>
					<li>FibrousMechanites</li>
					<li>SensoryMechanites</li>
					<li>Dementia</li>
					<li>Alzheimers</li>
					<li>Asthma</li>
					<li>HeartArteryBlockage</li>
					<li>Carcinoma</li>
					<li>HearingLoss</li>
					<li>Cataract</li>
					<li>BadBack</li>
					<li>Frail</li>
					<li>ResurrectionPsychosis</li>
				</explicitRemovals>
				<growthTicks>600</growthTicks>
				<growthText>Growth: </growthText>
				<protoBodyPart>Outland_ProtoBodypart</protoBodyPart>
			</li>
		</modExtensions>
	</GeneDef>

	<GeneDef>
		<defName>Outland_UnusualSpeech</defName>
		<label>unusual speech</label>
		<description>Carriers of this gene have an unusual way of talking, making it difficult for them to communicate with others.</description>
		<iconPath>OuterRim/GeneIcons/Gene_UnusualSpeech</iconPath>
		<displayCategory>Miscellaneous</displayCategory>
		<displayOrderInCategory>7905</displayOrderInCategory>
		<biostatCpx>1</biostatCpx>
		<biostatMet>1</biostatMet>
		<statOffsets>
			<SocialImpact>-0.2</SocialImpact>
		</statOffsets>
		<modExtensions>
			<li Class="VanillaGenesExpanded.GeneExtension">
				<backgroundPathEndogenes>OuterRim/GeneIcons/EndogeneBG</backgroundPathEndogenes>
				<backgroundPathXenogenes>OuterRim/GeneIcons/XenogeneBG</backgroundPathXenogenes>
			</li>
		</modExtensions>
	</GeneDef>

	<GeneDef>
		<defName>Outland_ThickSkin</defName>
		<label>thick skin</label>
		<description>Carriers of this gene have thick skin protecting them against blunt damage and some sharp damage.</description>
		<iconPath>OuterRim/GeneIcons/Gene_ThickSkin</iconPath>
		<displayOrderInCategory>7901</displayOrderInCategory>
		<displayCategory>Miscellaneous</displayCategory>
		<biostatCpx>1</biostatCpx>
		<biostatMet>-1</biostatMet>
		<statOffsets>
			<ArmorRating_Blunt>0.2</ArmorRating_Blunt>
		</statOffsets>
		<modExtensions>
			<li Class="VanillaGenesExpanded.GeneExtension">
				<backgroundPathEndogenes>OuterRim/GeneIcons/EndogeneBG</backgroundPathEndogenes>
				<backgroundPathXenogenes>OuterRim/GeneIcons/XenogeneBG</backgroundPathXenogenes>
			</li>
		</modExtensions>
	</GeneDef>

</Defs>